<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane id="AnchorPane" prefHeight="210.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lineage.controller.MainController">
   <children>
      <Pane prefHeight="210.0" prefWidth="320.0">
         <children>
            <VBox prefHeight="210.0" prefWidth="320.0">
               <children>
                  <Pane prefHeight="47.0" prefWidth="320.0">
                     <children>
                        <TextField fx:id="playerNameTextField" layoutX="9.0" layoutY="7.0" prefHeight="26.0" prefWidth="100.0" />
                        <Button fx:id="startBtn" layoutX="117.0" layoutY="7.0" mnemonicParsing="false" onAction="#startTask" prefHeight="26.0" prefWidth="87.0" text="Start" />
                        <Button fx:id="stopBtn" disable="true" layoutX="212.0" layoutY="7.0" mnemonicParsing="false" onAction="#stopTask" prefHeight="26.0" prefWidth="87.0" text="Stop" />
                     </children>
                  </Pane>
                  <Pane prefHeight="133.0" prefWidth="320.0">
                     <children>
                        <ListView fx:id="profilesListView" layoutX="10.0" prefHeight="126.0" prefWidth="162.0" />
                        <Button layoutX="202.0" layoutY="34.0" mnemonicParsing="false" onAction="#addProfile" prefHeight="26.0" prefWidth="100.0" text="Add" />
                        <Button layoutX="202.0" layoutY="66.0" mnemonicParsing="false" onAction="#editProfile" prefHeight="26.0" prefWidth="100.0" text="Edit" />
                        <Button layoutX="200.0" layoutY="99.0" mnemonicParsing="false" onAction="#removeProfile" prefHeight="26.0" prefWidth="100.0" text="Remove" />
                        <Button layoutX="202.0" layoutY="1.0" mnemonicParsing="false" onAction="#refreshProfile" prefHeight="26.0" prefWidth="100.0" text="Refresh" />
                     </children></Pane>
                  <Pane prefHeight="50.0" prefWidth="320.0">
                     <children>
                        <ComboBox fx:id="scriptsComboBox" layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="162.0" />
                        <TextField fx:id="winTextField" layoutX="202.0" layoutY="14.0" prefHeight="26.0" prefWidth="100.0" />
                     </children>
                  </Pane>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
